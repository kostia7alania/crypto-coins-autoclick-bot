{
  "name": "crypto-coins-autoclick-bot",
  "private": false,
  "version": "1.0.6",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "files": [
    "dist/*",
    "src"
  ],
  "lint-staged": {
    "*.{ts,js}": [
      "npm run pre-commit-ts",
      "npm run lint:prettier"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "test:unit": "vitest",
    "test:gitlab": "vitest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "lint:gitlab": "eslint --exit-on-fatal-error --format gitlab . --ext .ts",
    "lint:prettier": "prettier ./src -w --ignore-unknown",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "pre-commit-ts": "vue-tsc --noEmit && eslint --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/eslint": "^8.56.6",
    "@types/jsdom": "^21.1.6",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-formatter-gitlab": "5.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsonc": "2.14.1",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-unicorn": "51.0.1",
    "fast-glob": "^3.3.2",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-static-copy": "^1.0.1",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.7"
  },
  "engines": {
    "npm": ">=9.8.1",
    "node": ">=18.18.2"
  },
  "dependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^13.0.3",
    "@semantic-release/npm": "^12.0.0",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "semantic-release": "^23.0.5"
  }
}
